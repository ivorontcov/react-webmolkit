{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "strict": true,
    "jsx": "react-jsx",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "node",
    "lib": ["dom", "esnext"],
    "noEmitOnError": true,
    "baseUrl": "./src",
    "paths": {
      "*": ["./*"]
    }
  }
}
